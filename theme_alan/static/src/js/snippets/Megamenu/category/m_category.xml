<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <t t-name="theme_alan.m_category_configuration" owl="1">
        <div class="modal-body-in as_m_category_configuration">
            <SelectData search="this.props.search"
                init_record_ids="this.props.init_record_ids"
                frame_col_size="this.props.col_siz"
                selete_data_config="this.props.selete_data_config"
                quick_search="this.props.quick_search"
                design_and_edit="this.props.design_and_edit"
                slider_styles="this.props.slider_styles"
                grid_styles="this.props.grid_styles"
                extra_info="this.subCategory" />
        </div>
    </t>
    <t t-name="theme_alan.m_sub_category" owl="1">
        <Dialog title="this.title" header="this.header" footer="this.footer" technical="true" >
            <div class="container">
                <!-- <t t-esc="this.props.parent_id"/> -->
                <div class="as-mm-search-bar mb-4">
                    <input type="hidden" name="as_sub_category_search" id="as_sub_category_search" class="form-control" />
                </div>
                <button class="btn btn as-e-btn-success" t-on-click="_selectionDone"> <i class="fa fa-floppy-o"></i> Save </button>
            </div>
        </Dialog>
    </t>
</odoo>