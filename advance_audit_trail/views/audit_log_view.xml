<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- Inherited form view for auditlog.log -->
    <record model="ir.ui.view" id="view_auditlog_log_form_inherit">
        <field name="name">auditlog.log.form.inherit</field>
        <field name="model">auditlog.log</field>
        <field name="inherit_id" ref="auditlog.view_auditlog_log_form" />
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="ip_address" readonly="1" />
                <field name="platform_info" readonly="1" />
                <field name="terminal" readonly="1" />
                <field name="os_type" readonly="1" />
            </field>
        </field>
    </record>
    <!-- Inherited Tree view for auditlog.log -->
    <record model="ir.ui.view" id="view_auditlog_log_tree_inherit">
        <field name="name">auditlog.log.tree.inherit</field>
        <field name="model">auditlog.log</field>
        <field name="inherit_id" ref="auditlog.view_auditlog_log_tree" />
        <field name="arch" type="xml">
            <field name="user_id" position="after">
                <field name="ip_address" readonly="1" />
                <field name="platform_info" readonly="1" />
                <field name="terminal" readonly="1" />
                <field name="os_type" readonly="1" />
            </field>
        </field>
    </record>
</odoo>
